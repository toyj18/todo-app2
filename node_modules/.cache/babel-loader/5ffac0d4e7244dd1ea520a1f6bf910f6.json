AjBA,MAiBA;AACA;AACA,YAAA,SAAA,GAAA;AACA,UAAA,EAAA,EAAA,KAAA,EADA;AAEA,UAAA,KAAA,EAAA,KAAA,KAFA;AAGA,UAAA,OAAA,EAAA,KAAA;AAHA,SAAA,CAFA,CAOA;;AACA,aAAA,KAAA,CAAA,kBAAA,EAAA,SAAA,EARA,CASA;;AACA,aAAA,KAAA,GAAA,EAAA;AACA,aAAA,OAAA,GAAA,OAAA;AACA,aAAA,MAAA,GAAA,KAAA;AACA;AACA;AAvCA,GAXA;AAoDA,EAAA,KAAA,EAAA;AACA,IAAA,SAAA,EAAA;AACA,MAAA,OADA,qBACA;AACA,aAAA,EAAA,GAAA,KAAA,SAAA,CAAA,EAAA;AACA,aAAA,KAAA,GAAA,KAAA,SAAA,CAAA,KAAA;AACA,aAAA,OAAA,GAAA,KAAA,SAAA,CAAA,OAAA;AACA,aAAA,MAAA,GAAA,IAAA;AACA,OANA;AAOA,MAAA,IAAA,EAAA;AAPA,KADA;AAUA,IAAA,KAAA,EAAA;AACA,MAAA,OADA,qBACA;AACA,YAAA,KAAA,KAAA,KAAA,EAAA,EAAA;AACA,eAAA,MAAA,GAAA,KAAA;AACA;AACA;AALA;AAVA;AApDA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n\r\n\r\n        <form @submit=\"addEmail\">\r\n            <div class=\"form-group\">\r\n                <label for=\"email\">Enter Email</label>\r\n                <input type=\"email\" class=\"form-control\" id=\"email\" v-model=\"email\" name=\"email\" placeholder=\"Add Email\">\r\n            </div>\r\n            \r\n            <button type=\"submit\" class=\"btn btn-primary\">login</button>\r\n        </form>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        name: \"AddEmailItem\",\r\n        props: ['editEmail'],\r\n        data () {\r\n            return {\r\n                id: '',\r\n                email: '',\r\n                current: '',\r\n                status: false\r\n            }\r\n        },\r\n        methods: {\r\n            validEmail(email) {\r\n                var re = /^(([^<>()[\\]\\\\.,;:\\s@\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/;\r\n                return re.test(email);\r\n            },\r\n\r\n            addEmail(e){\r\n                e.preventDefault();\r\n                if (this.status === false){\r\n                    // add new Email\r\n                    const newEmail = {\r\n                        id: Math.floor(Math.random() * 100),\r\n                        email: this.email,\r\n                        current: new Date(),\r\n                        \r\n                    };\r\n                    if (!newEmail.email){\r\n                        alert('Email can not be empty');\r\n                        \r\n                    }else if (!this.validEmail(newEmail.email)) {\r\n                        alert(\"invalid email\")                        \r\n                    } else {\r\n                        this.$emit('add-email-event', newEmail);\r\n                    }\r\n                    this.email = ''\r\n                }else{\r\n                    //edit Email\r\n                    const EmailItem = {\r\n                        id: this.id,\r\n                        email: this.email,\r\n                        current: this.day,                        \r\n                    };\r\n                    //send to parent (App.vue)\r\n                    this.$emit('edit-email-event', EmailItem);\r\n                    // clear input field\r\n                    this.email = '';\r\n                    this.current = 'Email';\r\n                    this.status = false;\r\n                }\r\n            }\r\n        },\r\n        watch: {\r\n            editEmail: {\r\n                handler() {\r\n                    this.id = this.editEmail.id;\r\n                    this.email = this.editEmail.email;\r\n                    this.current = this.editEmail.current;\r\n                    this.status = true\r\n                },\r\n                deep: true\r\n            },\r\n            title:{\r\n                handler(){\r\n                    if(this.email === ''){\r\n                        this.status = false;\r\n                    }\r\n                }\r\n            }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"sourceRoot":"src/components"}]}
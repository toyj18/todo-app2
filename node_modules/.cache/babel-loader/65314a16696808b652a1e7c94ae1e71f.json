,KAAA;AACA,WAAA,QAAA,CAAA,GAAA,GAAA,KAAA,KAAA,CAAA,QAAA,EAAA,GAAA;AACA,WAAA,QAAA,CAAA,QAAA,GAAA,KAAA,KAAA,CAAA,QAAA,EAAA,QAAA;AAEA,KAjBA;AAkBA,IAAA,iBAlBA,6BAkBA,QAlBA,EAkBA;AACA;AACA,UAAA,QAAA,GAAA,KAAA,KAAA,CAAA,SAAA,CAAA,UAAA,GAAA;AAAA,eAAA,GAAA,CAAA,EAAA,KAAA,QAAA,CAAA,EAAA;AAAA,OAAA,CAAA,CAFA,CAGA;;AACA,WAAA,KAAA,CAAA,QAAA,EAAA,KAAA,GAAA,QAAA,CAAA,KAAA;AACA,WAAA,KAAA,CAAA,QAAA,EAAA,GAAA,GAAA,QAAA,CAAA,GAAA;AACA,WAAA,KAAA,CAAA,QAAA,EAAA,QAAA,GAAA,QAAA,CAAA,QAAA;AAEA;AA1BA,GAlBA;AA8CA,EAAA,KAAA,EAAA;AACA,IAAA,KAAA,EAAA;AACA,MAAA,OADA,qBACA;AACA,QAAA,YAAA,CAAA,OAAA,CAAA,OAAA,EAAA,IAAA,CAAA,SAAA,CAAA,KAAA,KAAA,CAAA;AACA,OAHA;AAIA,MAAA,IAAA,EAAA;AAJA;AADA,GA9CA;AAsDA,EAAA,OAtDA,qBAsDA;AACA,QAAA,YAAA,CAAA,OAAA,CAAA,OAAA,CAAA,EAAA;AACA,WAAA,KAAA,GAAA,IAAA,CAAA,KAAA,CAAA,YAAA,CAAA,OAAA,CAAA,OAAA,CAAA,CAAA;AACA;AACA;AA1DA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n        <div class=\"container\">\r\n      <div class=\"row\">\r\n        <div class=\"col-5 m-auto\">\r\n          <AddTaskItem \r\n          v-on:add-task-event=\"addTaskItem\" \r\n          v-on:edit-task-event=\"editTaskItemEvent\" \r\n          v-bind:editTask=\"editTask\"/>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    \r\n    <div>\r\n      <Tasks :tasks=\"tasks\" \r\n      @del-task-event=\"deleteTaskItem\" \r\n      @edit-task-event=\"editTaskItem\" />\r\n    </div>\r\n    </div>\r\n</template>\r\n<script>\r\n  import Tasks from \"./components/Tasks\";\r\n  import AddTaskItem from \"./components/AddTaskItem\";\r\n\r\nexport default {\r\n  name: 'TaskPage',\r\n  components: {\r\n    Tasks,\r\n    AddTaskItem\r\n  },\r\n  data () {\r\n    return {\r\n      tasks: [],\r\n      editTask: {\r\n        id: '',\r\n        title: '',\r\n        day: '',\r\n        reminder: ''\r\n        \r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    addTaskItem(newTask){\r\n      // console.log('newtask', newTask.title);\r\n        this.tasks = [...this.tasks, newTask];\r\n      // this.tasks.unshift(newTask)\r\n    },\r\n    deleteTaskItem(id){\r\n      this.tasks = this.tasks.filter(task => task.id !== id);\r\n    },\r\n    editTaskItem(id){\r\n      //find the index of the task's id\r\n      let objIndex = this.tasks.findIndex(obj=> obj.id === id);\r\n      this.editTask.id = id;\r\n      this.editTask.title = this.tasks[objIndex].title;\r\n      this.editTask.day = this.tasks[objIndex].day;\r\n      this.editTask.reminder = this.tasks[objIndex].reminder;\r\n      \r\n    },\r\n    editTaskItemEvent(taskItem){\r\n      //find the index of this id's object\r\n      let objIndex = this.tasks.findIndex(obj => obj.id === taskItem.id)\r\n      //update the item\r\n      this.tasks[objIndex].title = taskItem.title;\r\n      this.tasks[objIndex].day = taskItem.day;\r\n      this.tasks[objIndex].reminder = taskItem.reminder;\r\n      \r\n    }\r\n  },\r\n  watch: {\r\n    tasks: {\r\n      handler() {\r\n        localStorage.setItem('tasks',JSON.stringify(this.tasks))\r\n      },\r\n      deep: true\r\n    }\r\n  },\r\n  mounted() {\r\n    if (localStorage.getItem(\"tasks\")){\r\n      this.tasks = JSON.parse(localStorage.getItem(\"tasks\"))\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"sourceRoot":"src/components"}]}